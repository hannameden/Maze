<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hanna.maze">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo3"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.example.hanna.maze.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.hanna.maze.GameActivity"
            android:parentActivityName="com.example.hanna.maze.MainActivity">

        </activity>

        <activity android:name="com.example.hanna.maze.HighscoreActivity"
            android:parentActivityName="com.example.hanna.maze.MainActivity">

        </activity>

    </application>

</manifest>